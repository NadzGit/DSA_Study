def insertionSortC(A):
    B = A[:]

    for i in range (1, len(B)):
        j = i
        while j > 0 and B[j-1] > B[j]:
            B[j-1], B[j] = B[j], B[j-1]
            j -=1

        j += 1

    
    print(B)
    
    return B


insertionSortC([2,6,5,1,3,4])
insertionSortC([1,5,2,3,6,1,23])
insertionSortC([])
insertionSortC([5,1,2,90,1,3,45,2])
insertionSortC([2,8,1,20,422,12]) 